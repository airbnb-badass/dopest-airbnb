

  <div class="well">
    <h1>All the apartments :</h1>
  </div>

  <table class="table">
    <thead>
      <tr>
        <th>Title</th>
        <th>City</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <% @apartments.each do |apartment| %>
      <tr>
        <td><%= link_to apartment.title, apartment_path(apartment)%> </td>
        <td><%= apartment.city %></td>
        <td><%= apartment.price %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

