<table class="table">

  <thead>
    <tr>
      <th>Apartment</th>
      <th>Renter</th>
      <th>Checkin</th>
      <th>Checkout</th>
      <th>Guests</th>
      <th>status</th>
    </tr>
  </thead>

  <tbody>

  <% current_user.apartment_bookings.each do |booking| %>
    <tr>
      <td><%= booking.apartment.title %></td>
      <td><%= booking.user.email %></td>
      <td><%= booking.checkin %></td>
      <td><%= booking.checkout %></td>
      <td><%= booking.guests %></td>
      <td><%= booking.status %></td>
    </tr>
  <% end %>

  </tbody>
</table>